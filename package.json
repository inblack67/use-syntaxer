{
  "version": "0.1.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "use-syntaxer",
  "author": "Aman Bhardwaj",
  "repository": {
    "type": "git",
    "url": "https://github.com/inblack67/useMarkdown"
  },
  "homepage": "https://github.com/inblack67/useMarkdown",
  "keywords": [
    "react",
    "markdown",
    "syntax highlighting",
    "hook"
  ],
  "module": "dist/usemarkdown.esm.js",
  "size-limit": [
    {
      "path": "dist/usemarkdown.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/usemarkdown.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.6.0",
    "@types/react": "^16.9.50",
    "@types/react-dom": "^16.9.8",
    "@types/react-syntax-highlighter": "^11.0.5",
    "husky": "^4.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "size-limit": "^4.6.0",
    "tsdx": "^0.14.0",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "gray-matter": "^4.0.2",
    "react-markdown": "^4.3.1",
    "react-syntax-highlighter": "^15.0.1"
  }
}
